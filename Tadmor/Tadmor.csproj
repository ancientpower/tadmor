  </ItemGroup>

  <ItemGroup>
    <ApplicationDefinition Include="appsettings.sample.json">
      <CopyToOutputDirectory>Never</CopyToOutputDirectory>
    </ApplicationDefinition>
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Include="Resources\HelveticaNeueMedium.ttf">
